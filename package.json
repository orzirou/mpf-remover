{
  "name": "mpf-remover",
  "version": "0.0.0",
  "author": {
    "name": "orzirou"
  },
  "main": "./electron/main.js",
  "description": "This application makes electron & angular.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "package": "electron-builder -c ./projects/electron/config.json --mac --x64 --dir --win --x64 --dir",
    "build-r": "npm run build -- --configuration production --base-href ./",
    "build-m": "tsc -p ./projects/electron/tsconfig.json",
    "release": "rimraf ./dist && npm run build-r && npm run build-m && npm run package",
    "dev:build-r": "npm run build -- --configuration development --base-href ./",
    "dev:build-m": "webpack build --config ./webpack.dev.config.js --stats-error-details && cpx \"./node_modules/exiftool.pl/vendor/**/*\" ./dist/renderer/electron/vendor",
    "dev:release": "rimraf ./dist && npm run dev:build-r && npm run build-m && npm run package",
    "debug": "rimraf ./dist && npm run dev:build-r && npm run dev:build-m && electron ./dist/renderer/electron/main.js --trace-warnings"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "dist-exiftool": "^10.53.0",
    "lodash": "^4.17.21",
    "ngx-mat-counterspinner": "^14.0.2",
    "ngx-toastr": "^15.2.0",
    "node-exiftool": "^2.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "cpx": "^1.5.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^4.1.2",
    "ts-loader": "^9.4.2",
    "typescript": "~4.7.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
